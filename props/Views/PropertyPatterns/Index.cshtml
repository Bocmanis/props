@model IEnumerable<props.Models.PropertyPattern>

@{
    ViewBag.Title = "Index";
}

<h4>Patterns</h4>
<table class="table">

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.ActionLink(Html.DisplayFor(modelItem => item.Id).ToString(), "Details", new { id = item.Id })
        </td>
    </tr>
}

</table>
